<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TechProgMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TechProgMaui.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="TechProgMaui">

    <TabBar>
        <ShellContent
            Title="NewspaperPage"
            ContentTemplate="{DataTemplate local:NewspaperPage}"
            Route="MainPage" /> 
        <ShellContent
            Title="ParallelSort"
            ContentTemplate="{DataTemplate local:ParallelSortView}"
            Route="ParallelSortView" />
        <ShellContent
            Title="CancelSort"
            ContentTemplate="{DataTemplate local:CancelSortPage}"
            Route="CancelSortPage" />
    </TabBar>

</Shell>
